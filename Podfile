  pod 'RxCoreLocation', '~> 1.4'
  pod 'RxMKMapView', '~> 5.0'
  pod 'RxSwift', '~> 5.0'

  target 'FindEveryoneTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FindEveryoneUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
